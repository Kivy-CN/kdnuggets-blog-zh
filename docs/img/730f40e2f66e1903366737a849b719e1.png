<script>




// Get the radio buttons
const yesSrRadio = document.getElementById('yes-sr');
const noSrRadio = document.getElementById('no-sr');
const otherSrRadio = document.getElementById('other-sr');

// Add event listeners to the radio buttons
yesSrRadio.addEventListener('click', handleRadioClick);
noSrRadio.addEventListener('click', handleRadioClick);
otherSrRadio.addEventListener('click', handleRadioClick);

var textFields = document.querySelectorAll('input[type="text"][salaryInput="yes"]');
var dropdowns = document.querySelectorAll('select[salaryDropdown="no"]');

// //show and hide company input based on radio buttons official
function handleRadioClick() {
  const salaryYesElements = document.querySelectorAll('[salary=yes]');
  const salaryNoElements = document.querySelectorAll('[salary=no]');

  if (this.id === 'yes-sr') {
    // Hide salary=no elements and show salary=yes elements
    salaryNoElements.forEach(element => {
      element.style.display = 'none';
    });
    salaryYesElements.forEach(element => {
      element.style.display = 'block';
    });
    //make fields required and not required
    textFields.forEach(function (textField) {
      textField.required = true;
    });
    dropdowns.forEach(function (dropdowns) {
      dropdowns.required = false;
    });
  } else if (this.id === 'no-sr' || this.id === 'other-sr') {
    // Show salary=no elements and hide salary=yes elements
    salaryNoElements.forEach(element => {
      element.style.display = 'block';
    });
    salaryYesElements.forEach(element => {
      element.style.display = 'none';
    });
    //make fields required and not required
    textFields.forEach(function (textField) {
      textField.required = false;
    });
    dropdowns.forEach(function (dropdowns) {
      dropdowns.required = true;
    });
  }
}




//show and hide company input based on radio buttons
$(document).ready(function(){
  // Initially hide the div
  $('#other-div').hide();

  // Show the div when 'Yes' is clicked
  $('#yes-sr').click(function(){
    $('#company-div').show();
    $('#other-div').hide();
  });

  // Hide the div when 'No' is clicked
  $('#no-sr').click(function(){
    $('#company-div').hide();
    $('#other-div').hide();
  });
  
  //
  $('#other-sr').click(function(){
    $('#other-div').show();
  });
  
  
});



</script>


